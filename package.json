{
  "name": "msl-syntax-highlighter",
  "displayName": "MSL Syntax Highlighter",
  "description": "Syntax highlighting for MSL code",
  "version": "0.0.7",
  "publisher": "LukasMandok",
  "repository": {
    "type": "git",
    "url": "https://github.com/LukasMandok/msl-syntax-highlighter.git"
  },
  "bugs": {
    "url": "https://github.com/LukasMandok/msl-syntax-highlighter/issues"
  },
  "homepage": "https://github.com/LukasMandok/msl-syntax-highlighter#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "msl",
      "aliases": ["Midas Script Language", "msl", "MSL"],
      "extensions": [".msl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "msl",
      "scopeName": "source.msl",
      "path": "./syntaxes/msl.tmLanguage.json"
    }]
  }
}
